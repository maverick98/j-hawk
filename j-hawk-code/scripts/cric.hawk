function main()
{
    exec analyze()
}
function analyze()
{
   var playerName = "SR Tendulkar"
e  echo "analyzing player "+playerName
   exec analyzePlayer(playerName)
   playerName = "BC Lara"
   echo "analyzing player "+playerName	   
   exec analyzePlayer(playerName)
}
function analyzePlayer(var playerName)
{
    var respectableStrikeRate = 100.0
    var respectableStrikeRateInnings[] = `exec InnngFetcher->moreThanStrikeRate(respectableStrikeRate)`
    var playerWithRespectableStrikeRate[] =  `exec InningFetcher->equalPlayerName(respectableStrikeRateInnings,playerName)` 
    exec show(playerWithRespectableStrikeRate)
}
function show(var innings[])
{
    for(var i=1;i<=`exec SystemModule->length(innings)`;i=i+1)
    {
		echon innings[i] 
		/**
		* This will print details as you see in cricinfo
		* 
		*/
    }
}