function power(var x , var y)
{
    if(y == 0)
    {
        return 1
    }
   // echo  " y is "+y 
    var product = x
    for(var i=1;i<y ; i = i+1)
    { 
     //  echo "product is ::::" + product
     //  echo " x is "+x 
       product = product * x
    }
   // echo " power is "+product
    return product
}

